    <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
  </div>
  <div class="row">
    <div class="col-md-12">
      <nav class="navbar navbar-expand-md navbar-dark bg-dark" style="z-index: 5;">
        <div class="container-fluid">
          <a class="navbar-brand" href="/"><b>ConEdu</b></a>
          <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
          </button>
          <div class="collapse navbar-collapse justify-content-end" id="navbarNav">
            <ul class="navbar-nav">
              <li class="nav-item">
                <a class="nav-link" href="#">For Students</a>
              </li>
              
              <li class="nav-item">
                  <a class="nav-link" href="/user/yourtalkdesk">Your TalkDesk</a>
              </li>
              <li class="nav-item dropdown">
                  <a class="nav-link dropdown-toggle" href="#" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                      you
                  </a>
                  <ul class="dropdown-menu">
                      
                      <li><a class="dropdown-item" style="color: black; margin-left: 7%;" href="/user/profile">Profile</a></li>
                      <li><hr class="dropdown-divider"></li>
                      <li class="nav-item">
                          <a class="nav-link" style="color: black; margin-left: 7%;" href="/user/logout_welcome">Logout</a>
                      </li>
                  </ul>
                </li>
            </ul>
          </div>
        </div>
      </nav>
    </div>
    <div class="col-md-8 offset-md-2 info">
      <div class="jumbotron">
        <h1 class="display-4" style="text-align: center; color: black;">Your Profile</h1>
        <p class="lead" style="text-align: center; color: black;">
          <br>
        <div class="container" style="color: black;">
          <h1 style="color: black;text-align: right;"><%= student.name %></h1>
          <p><strong>School: <%= student.school %></strong></p>
          <p><strong>Achievements: <%= student.achievements %></strong></p>
          <p><strong>About: <%= student.about %></strong></p>
          <p><strong>Current Score: <%= student.score %></strong></p>
          <p><strong>Date Created: <%= student.datecreated %></strong></p>
          <p><strong>Date Updated: <%= student.dateupdated %></strong></p>
        </div>
      </p>
        <a class="btn btn-primary btn-lg" href="/user/choice" role="button">Update</a>
      </div>
    </div>
  </div>
</div>
